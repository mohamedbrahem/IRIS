<Page
    x:Class="IRIS.ImagineCup.Guide"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IRIS.ImagineCup"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Name="gridBack">
        <Grid.Background>
            <ImageBrush Stretch="UniformToFill" ImageSource="Assets/abc.png"/>
        </Grid.Background>

        <Image Name="brahem" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        <CommandBar VerticalAlignment="Bottom" Foreground="White">
            <CommandBar.Background>
                <ImageBrush Stretch="Fill" />
            </CommandBar.Background>
            <AppBarButton Icon="Back" Foreground="White" Click="AppBarButton_Click" />
            <AppBarButton Icon="Forward" Foreground="White" Click="AppBarButton_Click_1" ></AppBarButton>
            <AppBarButton Icon="Home" Foreground="White" Click="AppBarButton_Click_2" HorizontalAlignment="Center" VerticalAlignment="Top"></AppBarButton>
        </CommandBar>
    </Grid>
</Page>
