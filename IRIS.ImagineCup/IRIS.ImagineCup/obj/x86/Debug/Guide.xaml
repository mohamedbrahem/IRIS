<Page
    x:Class="IRIS.ImagineCup.Guide"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IRIS.ImagineCup"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid x:ConnectionId='1' Name="gridBack">
        <Grid.Background>
            <ImageBrush Stretch="UniformToFill" ImageSource="Assets/abc.png"/>
        </Grid.Background>

        <Image x:ConnectionId='2' Name="brahem" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        <CommandBar VerticalAlignment="Bottom" Foreground="White">
            <CommandBar.Background>
                <ImageBrush Stretch="Fill" />
            </CommandBar.Background>
            <AppBarButton x:ConnectionId='3' Icon="Back" Foreground="White"                            />
            <AppBarButton x:ConnectionId='4' Icon="Forward" Foreground="White"                              ></AppBarButton>
            <AppBarButton x:ConnectionId='5' Icon="Home" Foreground="White"                              HorizontalAlignment="Center" VerticalAlignment="Top"></AppBarButton>
        </CommandBar>
    </Grid>
</Page>

